<div id="trash" style="height: 200px; width: 100%; border: thin solid red;"></div>

<ul>

<% @notes.each do |n| %>
  <%= render 'one_note', :note => n  %>
<% end %>

</ul>

<%= form_for(Note.new, remote: true) do |f| %>
  <%= f.text_field :content, placeholder: 'Add a new Aha! here' %>
<% end %>

<%= subscribe_to "/notes/new" %>
